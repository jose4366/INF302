<?xml version="1.0" encoding="utf-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:CambiaColores"
    x:Class="CambiaColores.AppShell"
    Shell.BackgroundColor="White"
    Shell.NavBarHasShadow="False"
    Shell.TitleColor="#FF8B7BEF"
    Shell.DisabledColor="Gray"
    Shell.UnselectedColor="White"
    Shell.ForegroundColor="White"
    Shell.TabBarBackgroundColor="LightBlue"
    Shell.TabBarTitleColor="White"
    Shell.TabBarUnselectedColor="LightGray">

    <ShellContent Title="Inicio" ContentTemplate="{DataTemplate pages:MainPage}" />
    
</Shell>